{
	"name": "nindo-monorepo",
	"private": true,
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "yarn@3.2.1",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "yarn workspace @commonninja/nindo-storybook run storybook",
		"format": "prettier --write --ignore-unknown .",
		"format:check": "prettier --check --ignore-unknown .",
		"build": "yarn workspace @commonninja/nindo-storybook run build-storybook"
	},
	"devDependencies": {
		"prettier": "^2.5.1",
		"typescript": "^4.5.4"
	}
}
