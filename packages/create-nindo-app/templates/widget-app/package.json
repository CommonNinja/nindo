{
	"name": "nindo-app",
	"version": "1.0.0",
	"private": true,
	"proxy": "http://localhost:5006/",
	"engines": {
		"npm": "^6 || ^7 || ^8",
		"node": "^12 || ^14 || ^16 || ^18"
	},
	"scripts": {
		"start": "cn-scripts start",
		"build": "cn-scripts build",
		"test": "cn-scripts test",
		"analyze": "cn-scripts analyze",
		"simulate": "cn-scripts simulate",
		"postbuild": "cn-scripts post-build",
		"prettier": "prettier --write ."
	},
	"dependencies": {
		"@commonninja/nindo": "^1.1.0",
		"sass": "^1.35.1",
		"typescript": "^3.9.9"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,md,html,css,scss}": [
			"npm run prettier"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"husky": "^4.3.8",
		"lint-staged": "^10.5.4",
		"prettier": "^2.2.1"
	}
}
